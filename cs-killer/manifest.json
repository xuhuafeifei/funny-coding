{
    "manifest_version": 3,
    "name": "Bing 搜索优化",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["storage"],
    "content_scripts": [{
      "matches": ["https://www.bing.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }]
  }